      <script>var dbConn;
var result;
var table;

//var rootElement;
//var filteredRootElement;

var list = new java.util.ArrayList();

try {
	dbConn = DatabaseConnectionFactory.createDatabaseConnection(&apos;com.mysql.jdbc.Driver&apos;,globalMap.get(&quot;OPENHDS_DATABASE_URL&quot;), globalMap.get(&quot;OPENHDS_DATABASE_USER&quot;),globalMap.get(&quot;OPENHDS_DATABASE_PASSWORD&quot;));
	result = dbConn.executeCachedQuery(&quot;SELECT uuid, formName, active, deleted, coreTable, status FROM form where deleted=0 and status=2&quot;);

	while(result.next()){
			var coreTable = result.getString(&quot;coreTable&quot;);
			var uuid = result.getString(&quot;uuid&quot;);
			var formName = result.getString(&quot;formName&quot;);
			var active = result.getString(&quot;active&quot;);
			var deleted = result.getBoolean(&quot;deleted&quot;);
			var status = result.getInt(&quot;status&quot;);

			if((coreTable &amp;&amp; coreTable.trim().length() &gt; 0) &amp;&amp; !deleted 
						&amp;&amp; (active != null &amp;&amp; active.equalsIgnoreCase(&quot;yes&quot;))
						&amp;&amp; status == 2)
			{
				//logger.info(&quot;status 2, Proceed...&quot; + formName)
				list = readDataXml(formName, coreTable);
				/*
				logger.info(&quot;Entries in list: &quot; + list.size());
				for(var i = 0; i &lt; list.size(); i++){
					logger.info(&quot;List entry: &quot; + list.get(i).toXMLString());
				}
				*/
			}
	}
		//logger.info(&quot;List size: &quot; + list.size());
	return list;
} 
finally {
	if (dbConn) { 
		dbConn.close();
	}
	if (result) { 
		result.close();
	}
}</script>
